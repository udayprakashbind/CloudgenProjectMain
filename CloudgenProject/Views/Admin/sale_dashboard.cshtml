<script>
    function filterTableByDate() {
        var selectedDate = document.getElementById("followUpDate").value;
        var tableRows = document.querySelectorAll("#example1 tbody tr");

        tableRows.forEach(function (row) {
            var nextFollowUpDate = row.cells[9].innerText; // Assuming next_follow_up_date is in the 9th column
            if (nextFollowUpDate === selectedDate) {
                row.style.display = ""; // Show the row
            } else {
                row.style.display = "none"; // Hide the row
            }
        });
    }
</script>
